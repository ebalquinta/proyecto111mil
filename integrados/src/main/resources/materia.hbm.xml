<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">

<hibernate-mapping>
    <class name="org.integrados.data.actividad.Materia" table="Materia">
        <id name="id" type="java.lang.Integer" column="id" >
            <generator class="identity" />
        </id> 
        <property name="materia" type="string"  column="materia"/>
    </class>
</hibernate-mapping>
