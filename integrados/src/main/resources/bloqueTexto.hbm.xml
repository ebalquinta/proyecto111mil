<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="org.integrados.data.bloques.BloqueTexto" table="bloque" catalog="integrados_db" optimistic-lock="version">
        <id name="id" type="java.lang.Integer" column="id" >
            <generator class="identity" />
        </id>
        <property name="tipoBloque" type="integer" column="tipo_bloque" not-null="true" />
        <property name="texto" type="string" column="texto" />

  </class>
</hibernate-mapping>
