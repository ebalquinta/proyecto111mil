<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping.dtd">

<hibernate-mapping>
  <class name="org.integrados.data.bloques.FiltroBloque" table="bloque_and">
  <id name="id" type="java.lang.Integer" column="id" >
    <generator class="identity" />
  </id> 
  <many-to-one name="bloque1" class="org.integrados.data.bloques.Bloque" column="id_Bloque1" />
  <many-to-one name="bloque2" class="org.integrados.data.bloques.Bloque" column="id_Bloque2"/>
  <many-to-one name="bloqueAnd1" class="org.integrados.data.bloques.Bloqueand" column="id_BloqueAnd_1" />
  <many-to-one name="bloqueAnd2" class="org.integrados.data.bloques.Bloqueand" column="id_BloqueAnd_2"/>
  </class>
</hibernate-mapping>
